{
    "name": "uccello/package-designer",
    "description": "Adds a command to 'php artisan' to generate new packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "uccello/uccello": "1.*|2.*",
        "uccello/package-skeleton": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\PackageDesigner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\PackageDesigner\\Providers\\AppServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
